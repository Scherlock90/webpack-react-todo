{
  "name": "webpack-react-todo",
  "version": "1.0.0",
  "description": "First application in webpack",
  "repository": "https://github.com/Scherlock90/webpack-react-todo.git",
  "author": "Sebastian Napora",
  "license": "MIT",
  "private": true,
  "homepage": "https://Scherlock90.github.io/webpack-react-todo/",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.yarnpkg.com"
  },
  "scripts": {
    "clean": "rimraf build",
    "build:dev": "yarn run clean && webpack --config webpack.config.js --module-bind js=babel-loader",
    "start:dev": "webpack-dev-server --watch-content-base",
    "build": "webpack --config webpack.prod.js",
    "build:production": "webpack --config webpack.prod.js --module-bind js=babel-loader",
    "deploy": "gh-pages -d /build"
  },
  "dependencies": {
    "gh-pages": "^2.1.1",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.1",
    "webpack": "^4.39.3",
    "webpack-dev-server": "^3.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@types/core-js": "^2.5.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "history": "^4.9.0",
    "html-webpack-plugin": "^3.2.0",
    "manage-htaccess": "^1.0.0",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.1.0",
    "typescript": "^3.6.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-cli": "^3.3.8",
    "webpack-merge": "^4.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
